{
  "version": 2,
  "builds": [
    {
      "src": "./src/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "./src/api/index.js"
    },
    {
      "src": "/answers",
      "methods": ["POST"],
      "dest": "./src/api/index.js"
    }
  ]
}